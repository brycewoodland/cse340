<% if (title) { %>
    <h1><%= title %></h1>
<% } else {
    res.redirect('/')
} %>
    
<%- messages() %>

<% if (errors) { %>
    <ul class="notice">
<% errors.array().forEach(error => { %>
     <li><%= error.msg %></li>
<%  }) %>
   </ul>
<% } %>

<%- vehicleView %>

<form action="/account/approve-inventory/<%= inv_id %>" method="POST">
    <button type="submit">Accept</button>
</form>
<form action="/account/reject-inventory/<%= inv_id %>" method="POST">
    <button type="submit">Reject</button>
</form>